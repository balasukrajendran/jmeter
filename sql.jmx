          <stringProp name="query">-- Declare a variable to hold the name of the table to be updated
DECLARE @TableName NVARCHAR(128) = &apos;t_payors&apos;

-- Declare a variable to hold the name of the column to be updated
DECLARE @ColumnName NVARCHAR(128) = &apos;email&apos;

-- Declare a variable to build the dynamic SQL statement
DECLARE @SQL NVARCHAR(MAX)

-- Build the update SQL statement using the table and column names,
-- and set the column to its existing value for a random row
SET @SQL = &apos;UPDATE &apos; + @TableName +
           &apos; SET &apos; + @ColumnName + &apos; = &apos; + @ColumnName +
--         &apos; WHERE ID = (SELECT TOP 1 ID FROM &apos; + @TableName + &apos; ORDER BY NEWID())&apos;
           &apos; WHERE ID = (SELECT TOP 1 ID FROM &apos; + @TableName + &apos; TABLESAMPLE (1000 ROWS))&apos;
--           &apos; WHERE ID = 14579873&apos;
--          &apos; WHERE ID =  ( SELECT top 1 id FROM t_payors   TABLESAMPLE (1000 ROWS))&apos;

-- Execute the update SQL statement
EXEC sp_executesql @SQL

-- Get the number of rows updated
DECLARE @RowCount INT
SET @RowCount = @@ROWCOUNT

-- Output the number of rows updated
SELECT @RowCount AS RowsUpdated</stringProp>
